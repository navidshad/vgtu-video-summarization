{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Electron: All",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev", "--", "--sourcemap", "--debug"],
      "outputCapture": "std",
      "autoAttachChildProcesses": true,
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}